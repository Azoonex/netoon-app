{
  "expo": {
    "name": "rn-starter",
    "slug": "rn-starter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-android.png",
    "scheme": "rnstarter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon-android.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.azoonex.rnstarter"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon-android.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "03adc445-556d-46a2-9904-b30efd262b00"
      }
    }
  }
}
